{
  "name": "mass-tools",
  "version": "0.55.0",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "cheminfo build -r MolecularFormula && npm run build-docs",
    "build-docs": "ls packages | xargs -I % sh -c 'cd packages/%; documentation build src/index.js -f html -o ../../docs/%; cd ../..'",
    "eslint": "eslint packages --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "lerna bootstrap --no-ci",
    "publish": "git pull --rebase && npm run test && lerna publish",
    "test": "jest && npm run eslint",
    "test-coverage": "jest --coverage",
    "upgrade": "find packages -type f -name 'package.json' -maxdepth 2 -execdir ncu --upgrade \\;"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheminfo/mf-parser.git"
  },
  "author": "Luc Patiny <luc@patiny.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/mf-parser/issues"
  },
  "homepage": "https://github.com/cheminfo/mf-parser#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "benchmark": "^2.1.4",
    "chemcalc": "^3.4.1",
    "chemical-elements": "^1.1.5",
    "cheminfo-build": "^1.1.6",
    "codecov": "^3.7.2",
    "documentation": "^13.0.2",
    "eslint": "^7.10.0",
    "eslint-config-cheminfo": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jest-matcher-deep-close-to": "^2.0.1",
    "lerna": "^3.22.1",
    "mf-parser": "^1.1.6",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.3.0",
    "prettier": "^2.1.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "wikidata-sdk": "^7.7.1",
    "xy-parser": "^3.0.0"
  },
  "dependencies": {
    "peaks-similarity": "^2.3.1",
    "peptide": "^1.10.1",
    "sum-object-keys": "^1.0.2"
  }
}

{
  "private": true,
  "name": "molecular-formula",
  "version": "0.3.5",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "cheminfo build && npm run build-docs",
    "build-docs": "ls packages | xargs -I % sh -c 'cd packages/%; documentation build src/index.js -f html -o ../../docs/%; cd ../..'",
    "eslint": "eslint packages/*.js --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "lerna bootstrap",
    "publish": "git pull --rebase && npm run test && lerna publish",
    "test": "npm run testonly && npm run eslint",
    "test-travis": "eslint src && jest --coverage && codecov",
    "testonly": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheminfo-js/mf-parser.git"
  },
  "author": "Luc Patiny <luc@patiny.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo-js/mf-parser/issues"
  },
  "homepage": "https://github.com/cheminfo-js/mf-parser#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "benchmark": "^2.1.4",
    "chemcalc": "^3.4.1",
    "cheminfo-tools": "^1.21.0",
    "codecov": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-no-only-tests": "^2.0.1",
    "jest": "^22.4.4",
    "lerna": "^2.11.0",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "documentation": "^6.3.3",
    "peptide": "^1.6.1",
    "sum-object-keys": "^1.0.2"
  }
}
